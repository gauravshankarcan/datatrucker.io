{
      "/utils/fuzzy": {
            "trucker-page-title": "Create Fuzzy Search  API",
            "trucker-page-createtableheading": "Create an Fuzzy Search API",
            "createapi": "resources",
            "deleteapi": "resources",
            "deleteid": "filename",
            "searchapi": "resources?type=Util-Fuzzy",
            "getapi": "resources",
            "searchcolumns": [
                  {
                        "field": "restmethod",
                        "title": "Method"
                  },
                  {
                        "field": "tenant",
                        "title": "Tenant"
                  },
                  {
                        "field": "resourcename",
                        "title": "Resource Name"
                  },
                  {
                        "field": "filename",
                        "title": "Filename"
                  }
            ],
            "create": [
                  {
                        "field": "resourcename",
                        "placeholder": "Resource Name",
                        "type": "text",
                        "locked": false
                  },
                  {
                        "field": "restmethod",
                        "placeholder": "Method",
                        "type": "dropdown",
                        "locked": false,
                        "values": ["GET", "POST", "PUT", "DELETE", "PATCH"],
                        "defaultValue": "POST"
                  },
                  {
                        "field": "type",
                        "placeholder": "Type",
                        "type": "text",
                        "locked": true,
                        "defaultValue": "Util-Fuzzy"
                  },
                  {
                        "field": "options",
                        "placeholder": "Options",
                        "type": "jsonarea",
                        "locked": false,
                        "defaultValue": {
                              "keys": ["author"]
                        }
                  },
                  {
                        "field": "validations",
                        "placeholder": "Validations",
                        "type": "jsonarea",
                        "locked": false,
                        "defaultValue": {
                              "type": "object",
                              "properties": {
                                    "key": {
                                          "type": "string",
                                          "pattern": "^[a-z0-9]*$",
                                          "maxLength": 5
                                    }
                              }
                        }
                  }
            ]
      },
