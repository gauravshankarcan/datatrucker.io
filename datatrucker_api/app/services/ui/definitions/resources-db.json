                  },
                  {
                        "field": "tenant",
                        "title": "Tenant"
                  },
                  {
                        "field": "resourcename",
                        "title": "Resource Name"
                  },
                  {
                        "field": "filename",
                        "title": "Filename"
                  }
            ],
            "create": [
                  {
                        "field": "resourcename",
                        "placeholder": "Resource Name",
                        "type": "text",
                        "locked": false
                  },
                  {
                        "field": "credentialname",
                        "placeholder": "Credential Name",
                        "type": "text",
                        "locked": false
                  },
                  {
                        "field": "restmethod",
                        "placeholder": "Method",
                        "type": "dropdown",
                        "locked": false,
                        "values": ["GET", "POST", "PUT", "DELETE", "PATCH"],
                        "defaultValue": "POST"
                  },
                  {
                        "field": "job_timeout",
                        "placeholder": "Timeout",
                        "type": "integer",
                        "locked": false,
                        "defaultValue": 1000
                  },
                  {
                        "field": "script",
                        "placeholder": "Script",
                        "type": "textarea",
                        "locked": false,
                        "defaultValue": "Select * from table"
                  },
                  {
                        "field": "validations",
                        "placeholder": "Validations",
                        "type": "jsonarea",
                        "locked": false,
                        "defaultValue": {
                              "type": "object",
                              "properties": {
                                    "key": {
                                          "type": "string",
                                          "pattern": "^[a-z0-9]*$",
                                          "maxLength": 5
                                    }
                              }
                        }
                  },
                  {
                        "field": "type",
                        "placeholder": "Type",
                        "type": "text",
                        "locked": true,
                        "defaultValue": "DB-Postgres"
                  }
            ]
      },
