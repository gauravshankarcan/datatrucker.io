{
      "name": "datatrucker",
      "version": "1.0.0",
      "description": "",
      "main": "app.js",
      "directories": {
            "test": "test"
      },
      "scripts": {
            "migrate": "knex migrate:latest --env production",
            "rebuild": "knex migrate:rollback --env production && npm run migrate",
            "start": "npm run migrate && node app.js",
            "dev-app": "npm run migrate && nodemon app.js",
            "lint:services": "./node_modules/.bin/eslint --fix services",
            "lint:connectors": "./node_modules/.bin/eslint --fix connectors",
            "lint:middleware": "./node_modules/.bin/eslint --fix middleware",
            "lint:migrations": "./node_modules/.bin/eslint --fix migrations",
            "lint:plugins": "./node_modules/.bin/eslint --fix plugins",
            "lint:app": "./node_modules/.bin/eslint --fix app.js",
            "lint:knexfile": "./node_modules/.bin/eslint --fix knexfile.js",
            "lint": "npm run lint:services && npm run lint:connectors && npm run lint:middleware && npm run lint:plugins && npm run lint:app && npm run lint:knexfile",
            "pretty:nest1": "prettier --print-width 250 --single-quote --trailing-comma none --no-bracket-spacing --tab-width 6 --write **/**/*.js",
            "pretty:nest2": "prettier --print-width 250 --single-quote --trailing-comma none --no-bracket-spacing --tab-width 6 --write **/*.js",
            "pretty:root": "prettier --print-width 250 --single-quote --trailing-comma none --no-bracket-spacing --tab-width 6 --write *.js",
            "pretty": "npm run pretty:nest1 && npm run pretty:nest2 && npm run pretty:root"
      },
      "keywords": [],
      "author": "",
      "license": "ISC",
      "dependencies": {
            "ajv": "^8.6.2",
            "app-root-path": "^3.0.0",
            "fastify": "^3.20.2",
            "fastify-compress": "^3.6.0",
            "fastify-cookie": "^5.3.1",
            "fastify-helmet": "^5.3.2",
            "fastify-jwt": "^3.0.1",
            "fastify-plugin": "^3.0.0",
            "fastify-sensible": "^3.1.1",
            "fastify-session": "^5.2.1",
            "fuse.js": "^6.4.6",
            "jsonpath": "^1.1.1",
            "jsonwebtoken": "^8.5.1",
            "kafkajs": "^1.15.0",
            "keycloak-connect": "^15.0.2",
            "knex": "^0.95.11",
            "lodash": "^4.17.21",
            "momentjs": "^2.0.0",
            "mssql": "^7.2.1",
            "mysql": "^2.18.1",
            "node-cache": "^5.1.2",
            "node-cron": "^3.0.0",
            "node-fetch": "^2.6.1",
            "node-ssh": "^12.0.0",
            "oracledb": "^5.2.0",
            "pg": "^8.7.1",
            "redis": "^3.1.2",
            "sentiment": "^5.0.2",
            "shelljs": "^0.8.4",
            "sqlite3": "^5.0.2",
            "ssh2-sftp-client": "^7.0.3"
      },
      "devDependencies": {
            "chai": "^4.3.4",
            "chai-http": "^4.3.0",
            "eslint": "^7.32.0",
            "eslint-config-airbnb-base": "^14.2.1",
            "eslint-plugin-import": "^2.24.2",
            "pino-pretty": "^6.0.0",
            "tap": "^15.0.9"
      }
